import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        while(s.length()>0){
            char ch = s.charAt(0);
            int count = s.length() - s.replace(ch + "", "").length();
            System.out.println(ch+ "   " +count);
            s = s.replace(ch+"", "");
        }
    }
}
