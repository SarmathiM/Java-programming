import java.io.*;
import java.util.*;

public class Solution {
    static int counts(int n){
        if(n==0){
            return 0;
        }
        return 1 + counts(n/10);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if(n==0){
            System.out.println("The Count the digits: 1");
        }
        else{
            System.out.println("The Count the digits: "+ counts(Math.abs(n)));
        }
    }
}
