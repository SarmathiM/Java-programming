import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;
        if(n<=12){
            System.out.println("Invalid Input");
            return;
        }
        int temp = n;
        while(temp>0){
            sum+=temp%10;
            temp=temp/10;
        }
        System.out.println("Sum of digit is "+sum);
        }
        
    }
